<?xml version="1.0" encoding="utf-8"?>

<androidx.core.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:fitsSystemWindows="true">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        tools:context=".PhotoResultsActivity">

        <Toolbar
            android:id="@+id/photoResultToolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/black_transparent"
            android:minHeight="?attr/actionBarSize"
            android:title="@string/result"
            android:elevation="1dp">
        </Toolbar>

        <TextView android:id="@+id/SelfieTextView"
            android:layout_width="wrap_content"
            android:layout_marginTop="@dimen/dp_27"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:text="Selfie"/>

        <ImageView
            android:id="@+id/photoResultImageView"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_height="150dp" />

        <TextView android:id="@+id/realSpoofTextView"
            android:layout_width="wrap_content"
            android:layout_marginTop="@dimen/dp_27"
            android:layout_height="wrap_content"
            tools:text="REAL"/>

        <TextView android:id="@+id/faceMatchTextView"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_height="wrap_content"
            tools:text="FACE MATCHES"/>

        <TextView android:id="@+id/idfrontTextView"
            android:layout_width="wrap_content"
            android:layout_marginTop="@dimen/dp_27"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:text="Id front"/>

        <ImageView
            android:id="@+id/docFrontImageView"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_height="150dp" />


        <TextView android:id="@+id/idbackTextView"
            android:layout_width="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_27"
            android:text="Id back"/>
        <ImageView
            android:id="@+id/docBackImageView"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_height="150dp" />

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:id="@+id/ll_processed_id_data"
            android:orientation="vertical">
            <LinearLayout
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:id="@+id/ll_processed_full_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/full_name"
                        android:textAppearance="@style/TextAppearance.ResultTitle" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tv_processed_full_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="@string/full_name"
                        android:hint="@string/empty_data"
                        android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_marginStart="@dimen/dp_10"
                    android:orientation="vertical"
                    android:id="@+id/ll_processed_dob"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="end"
                        android:text="@string/dob"
                        android:textAppearance="@style/TextAppearance.ResultTitle" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tv_processed_dob"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="@string/dob"
                        android:hint="@string/empty_data"
                        android:gravity="end"
                        android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

                </LinearLayout>
            </LinearLayout>



            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/ll_processed_document"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <View
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:background="@color/color_839BB1"
                    android:layout_width="match_parent"
                    android:layout_height="0.4dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/document_number"
                    android:textAppearance="@style/TextAppearance.ResultTitle" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_processed_document"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="@string/document_number"
                    android:hint="@string/empty_data"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

            </LinearLayout>
            <LinearLayout
                android:weightSum="2"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:id="@+id/ll_processed_issue_date"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/id_issue_date"
                        android:textAppearance="@style/TextAppearance.ResultTitle" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tv_processed_issue_date"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:text="Name"
                        android:hint="@string/empty_data"
                        android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:id="@+id/ll_processed_expiry"
                    android:orientation="vertical"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/id_expiry_date"
                        android:layout_gravity="end"
                        android:textAppearance="@style/TextAppearance.ResultTitle" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tv_processed_expiry"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        tools:text="@string/id_expiry_date"
                        android:hint="@string/empty_data"
                        android:layout_gravity="end"
                        android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/ll_processed_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <View
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:background="@color/color_839BB1"
                    android:layout_width="match_parent"
                    android:layout_height="0.4dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/address"
                    android:textAppearance="@style/TextAppearance.ResultTitle" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_processed_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="@string/address"
                    android:hint="@string/empty_data"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium" />

            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/ll_processed_gender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:background="@color/color_839BB1"
                    android:layout_width="match_parent"
                    android:layout_height="0.4dp"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/gender"
                    android:textAppearance="@style/TextAppearance.ResultTitle" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_processed_gender"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="@string/gender"
                    android:hint="@string/empty_data"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

            </LinearLayout>

        </LinearLayout>


        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Server Response"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="16dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="22sp"
            />
        <ProgressBar
            android:id="@+id/indeterminateBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:layout_gravity="center_horizontal"
            />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginRight="@dimen/dp_16"
        android:id="@+id/ll_id_data"
        android:visibility="gone"
        android:orientation="vertical">
        <LinearLayout
            android:weightSum="2"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_weight="1"
                android:orientation="vertical"
                android:id="@+id/ll_full_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/full_name"
                    android:textAppearance="@style/TextAppearance.ResultTitle" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_full_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="@string/full_name"
                    android:hint="@string/empty_data"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_marginStart="@dimen/dp_10"
                android:orientation="vertical"
                android:id="@+id/ll_dob"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:text="@string/dob"
                    android:textAppearance="@style/TextAppearance.ResultTitle" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_dob"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="@string/dob"
                    android:hint="@string/empty_data"
                    android:gravity="end"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

            </LinearLayout>
        </LinearLayout>



        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/ll_document"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_15"
                android:background="@color/color_839BB1"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/document_number"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_document"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:text="@string/document_number"
                android:hint="@string/empty_data"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_15"
            android:background="@color/color_839BB1"
            android:layout_width="match_parent"
            android:layout_height="0.4dp"/>
        <LinearLayout
            android:weightSum="2"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_weight="1"
                android:orientation="vertical"
                android:id="@+id/ll_issue_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/id_issue_date"
                    android:textAppearance="@style/TextAppearance.ResultTitle" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_issue_date"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:text="Name"
                    android:hint="@string/empty_data"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:id="@+id/ll_expiry"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/id_expiry_date"
                    android:layout_gravity="end"
                    android:textAppearance="@style/TextAppearance.ResultTitle" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_expiry"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="@string/id_expiry_date"
                    android:hint="@string/empty_data"
                    android:layout_gravity="end"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/ll_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <View
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_15"
                android:background="@color/color_839BB1"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/address"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:text="@string/address"
                android:hint="@string/empty_data"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium" />

        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/ll_gender"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <View
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_15"
                android:background="@color/color_839BB1"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/gender"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_gender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:text="@string/gender"
                android:hint="@string/empty_data"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginRight="@dimen/dp_16"
        android:id="@+id/ll_enrolled_data"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_marginTop="@dimen/dp_15"
            android:layout_weight="1"
            android:visibility="gone"
            android:id="@+id/ll_enrolled_full_name"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/full_name"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_enrolled_full_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:text="@string/full_name"
                android:hint="@string/empty_data"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_15"
            android:background="@color/color_839BB1"
            android:layout_width="match_parent"
            android:layout_height="0.4dp"/>

        <LinearLayout
            android:orientation="vertical"
            android:visibility="gone"
            android:id="@+id/ll_enrolled_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/enrolled_date"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_enrolled_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:text="@string/enrolled_date"
                android:hint="@string/empty_data"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium"/>

        </LinearLayout>
        <View
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_15"
            android:background="@color/color_839BB1"
            android:layout_width="match_parent"
            android:visibility="gone"
            android:layout_height="0.4dp"/>

        <LinearLayout
            android:orientation="vertical"
            android:visibility="gone"
            android:id="@+id/ll_client_customer_number"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/client_customer_num"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_client_customer_number"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/empty_data"
                tools:text="@string/client_customer_num"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium" />

        </LinearLayout>
    </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/kyc_dmv_result"
            android:visibility="gone"
            android:layout_margin="@dimen/dp_16"
            android:layout_height="wrap_content">

            <View
                android:id="@+id/view_separator"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_15"
                android:background="@color/color_839BB1"
                android:layout_width="match_parent"
                android:layout_height="0.4dp"/>

            <TextView
                android:id="@+id/tv_dmv_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/view_separator"
                android:text="@string/dmv_result"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_dmv_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_dmv_title"
                android:layout_toEndOf="@id/iv_dmv_result"
                tools:text="@string/dmv_result"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium" />

            <ImageView
                android:id="@+id/iv_dmv_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_dmv_title"
                android:layout_marginStart="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginEnd="@dimen/dp_5"
                android:src="@drawable/circle_result"
                app:tint="@color/color_FF9F30" />

            <TextView
                android:id="@+id/tv_result_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_dmv_result"
                android:text="@string/aml_kyc_result"
                android:layout_marginTop="@dimen/dp_15"
                android:textAppearance="@style/TextAppearance.ResultTitle" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_kyc_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_result_title"
                android:layout_toEndOf="@id/iv_kyc_result"
                android:textAppearance="@style/TitleTextAppearance.RobotoMedium"
                tools:text="@string/aml_kyc_result" />

            <ImageView
                android:id="@+id/iv_kyc_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_result_title"
                android:layout_marginStart="@dimen/dp_5"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginEnd="@dimen/dp_5"
                android:src="@drawable/circle_result"
                app:tint="@color/color_FF9F30" />

            <RelativeLayout
                android:id="@+id/pep_status"
                android:layout_below="@+id/iv_kyc_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_11">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal">
                    <View
                        android:id="@+id/left_view1"
                        android:layout_width="@dimen/dp_1"
                        android:layout_height="@dimen/dp_35"
                        android:background="@color/color_707070" />


                    <View
                        android:id="@+id/right_view1"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_1"
                        android:layout_below="@id/left_view1"
                        android:background="@color/color_707070" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_kyc_result"
                android:layout_marginTop="@dimen/dp_18"
                android:layout_toEndOf="@id/pep_status"
                android:gravity="center_vertical">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/iv_pep_status"
                    android:layout_width="@dimen/dp_35"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_5"
                    android:text="NA"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"
                    android:textColor="@color/color_ad0000"
                    app:drawableTint="@color/color_ad0000" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_pep__status"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toEndOf="@id/iv_pep_status"
                    android:text="@string/pep"
                    android:layout_centerVertical="true"
                    android:textAppearance="@style/TitleTextAppearance.RobotoRegular" />
            </RelativeLayout>


            <RelativeLayout
                android:layout_below="@id/pep_status"
                android:id="@+id/wl_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_11">
                <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal">
                    <View
                        android:id="@+id/left_view2"
                        android:layout_width="@dimen/dp_1"
                        android:layout_height="@dimen/dp_35"
                        android:background="@color/color_707070" />


                    <View
                        android:id="@+id/right_view2"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_1"
                        android:layout_below="@id/left_view2"
                        android:background="@color/color_707070" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pep_status"
                android:gravity="center_vertical"
                android:layout_marginTop="@dimen/dp_18"
                android:layout_toEndOf="@id/wl_status">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/iv_wl_status"
                    android:layout_width="@dimen/dp_35"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_5"
                    android:text="NA"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"
                    android:textColor="@color/color_ad0000"
                    app:drawableTint="@color/color_ad0000" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_wl_status"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@id/iv_wl_status"
                    android:text="@string/watch_list"
                    android:textAppearance="@style/TitleTextAppearance.RobotoRegular" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/nm_status"
                android:layout_below="@+id/wl_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_11">
                <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal">
                    <View
                        android:id="@+id/left_view"
                        android:layout_width="@dimen/dp_1"
                        android:layout_height="@dimen/dp_35"
                        android:background="@color/color_707070" />


                    <View
                        android:id="@+id/right_view"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_1"
                        android:layout_below="@id/left_view"
                        android:background="@color/color_707070" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wl_status"
                android:layout_marginTop="@dimen/dp_18"
                android:layout_toEndOf="@id/nm_status">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_nm_status"
                    android:layout_width="@dimen/dp_35"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_5"
                    android:layout_marginTop="@dimen/dp_5"
                    android:text="NA"
                    android:textAppearance="@style/TitleTextAppearance.RobotoMedium"
                    android:textColor="@color/color_ad0000"
                    app:drawableTint="@color/color_ad0000" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_nm_status_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@id/tv_nm_status"
                    android:text="@string/negative_media"
                    android:textAppearance="@style/TitleTextAppearance.RobotoRegular" />
            </RelativeLayout>
        </RelativeLayout>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:id="@+id/ll_doc_image"
        android:layout_marginTop="@dimen/dp_15"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/ll_document_result"
            android:layout_width="match_parent"
            android:visibility="gone"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_doc_image"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_180"
                android:scaleX="1"
                android:scaleY="1"
                tools:src="@drawable/ic_no_face_detect_blue" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/rl_live_result"
            android:layout_gravity="center_horizontal"
            android:visibility="gone"
            android:layout_width="@dimen/dp_160"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:layout_width="@dimen/dp_135"
                    android:layout_height="@dimen/dp_150"
                    android:background="@color/white"
                    app:cardCornerRadius="@dimen/dp_10">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/live_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/ic_no_face_detect_blue" />
                </androidx.cardview.widget.CardView>
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt_done"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_28"
            android:backgroundTint="@color/color_1C2B48"
            android:padding="@dimen/dp_12"
            android:text="@string/done"
            app:cornerRadius="@dimen/dp_5"
            android:textColor="@color/white"
            android:layout_marginStart="@dimen/dp_9"
            android:layout_marginEnd="@dimen/dp_9"
            style="@style/App.PrimaryButtonText" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>
